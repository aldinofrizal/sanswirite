n          </div>\n        </div>\n\n        <form class=\"ui form\">\n          <div class=\"field\">\n            <label>Username</label>\n            <input\n              type=\"text\"\n              name=\"first-name\"\n              placeholder=\"username\"\n              autocomplete=\"off\"\n              v-model=\"form.username\"\n            />\n          </div>\n          <div class=\"field\">\n            <label>Email</label>\n            <input\n              type=\"email\"\n              name=\"first-name\"\n              placeholder=\"email\"\n              autocomplete=\"off\"\n              v-model=\"form.email\"\n            />\n          </div>\n          <div class=\"field\">\n            <label>Password</label>\n            <input\n              type=\"password\"\n              name=\"first-name\"\n              placeholder=\"password\"\n              autocomplete=\"off\"\n              v-model=\"form.password\"\n            />\n          </div>\n          <button class=\"ui orange button\" type=\"submit\" @click.prevent=\"register()\">register</button>\n          <div class=\"ui active inline loader\" v-if=\"isLoading\"></div>\n        </form>\n        <p>\n          member of Sanswrite? click to\n          <a style=\"cursor: pointer;\" @click.prevent=\"toLogin()\">login!</a>\n        </p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport swal from \"sweetalert\"\n\nexport default {\n  name: \"Register\",\n  data() {\n    return {\n      isLoading: false,\n      form: {\n        username: \"\",\n        email: \"\",\n        password: \"\",\n        err: []\n      }\n    };\n  },\n  methods: {\n    toLogin() {\n      this.$emit(\"toggleForm\", true);\n    },\n    register() {\n      this.isLoading = true;\n      axios({\n        method: \"post\",\n        url: \"http://34.87.84.2/user/register\",\n        data: {\n          username: this.form.username,\n          email: this.form.email,\n          password: this.form.password,\n          err: []\n        }\n      })\n        .then(userCreated => {\n          setTimeout(() => {\n            this.isLoading = false;\n            swal(\"Successfully register! please login to join the party!\");\n            this.toLogin();\n          }, 1000);\n        })\n        .catch(err => {\n          setTimeout(() => {\n            this.isLoading = false;\n            this.form.err = err.response.data.msg;\n          }, 1000);\n        });\n    }\n  }\n};\n</script>\n\n<style scoped>\n#landingpage {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  height: 100vh;\n}\n#landingpage p {\n  margin-top: 3vh;\n}\n#landingpage a:hover {\n  font-size: 15px;\n}\n#loginform {\n  width: 100vh;\n  height: 70vh;\n  padding: 9vh 15vh;\n  border-radius: 5px;\n  /* background-color: #2274A5; */\n}\n#landingpage #registerform {\n  height: 70vh;\n}\n.ui.error.message {\n  margin-bottom: 2vh;\n}\n</style>"},"lineCount":null}},"error":null,"hash":"d1b50795169fbdcf501ce7a6a7984a37","cacheData":{"env":{}}}